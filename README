## KEPPLER CMS

# inputs examples

file: = f.input :name, :as => :file_material, label: false, wrapper_html: { class: "file-field" }
select: = f.input :role_ids, collection: Role.all, label: false, include_blank: "Selecione un rol"
radio_buttons: = f.collection_radio_buttons :name, [['vegan', 'vegan'] ,['vegetarian', 'vegetarian']],:first, :last
checkbox: = f.collection_check_boxes :name, [['vegan', 'vegan'] ,['vegetarian', 'vegetarian']],:first, :last